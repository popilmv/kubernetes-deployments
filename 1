apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: my-ingress27
  labels:
    name: my-ingress27
  annotations:
    cert-manager.io/issuer: letsencrypt
spec:
  ingressClassName: nginx
  rules:
  - host: m.int.itoutposts.com
    http:
      paths:
      - pathType: Prefix
        path: /
        backend:
          service:
            name: my-pod-service  
            port: 
              number: 80
  tls:
  - hosts:
    - m.int.itoutposts.com
    secretName: ssl-lets

          
          
          
